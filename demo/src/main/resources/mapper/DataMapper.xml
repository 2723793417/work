<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.xnjt.demo.mapper.DataMapper">
	<insert id="insertMoviename" parameterType="edu.xnjt.demo.model.Data">
	      insert into lovemovie
	      (mname,starring,director,into,imgurl)
	      values
	      (#{mname},#{starring},#{director},#{into},#{imgurl})
	</insert>
	
            <select id="selectMoviename" parameterType="java.lang.String" resultType="edu.xnjt.demo.model.Data">
              select * from lovemovie
              where
              mname = #{mname}
            </select>
</mapper>
